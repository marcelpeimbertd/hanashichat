{
  "name": "hanashichat",
  "version": "0.0.5",
  "description": "A chat to interact with new people or old friends",
  "main": "/dist/server/index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/server",
    "start-development": "nodemon dist/server --watch dist",
    "build": "webpack && tsc -p tsconfig.server.json",
    "build-watch": "concurrently \"mongod \" \"webpack -w\" \"tsc -p tsconfig.server.json -w\" \"nodemon -r dotenv/config dist/server --watch dist\"",
    "build-watchdebbug": "concurrently \"mongod \" \"webpack -w\" \"tsc -p tsconfig.server.json -w\" \"nodemon -r dotenv/config --inspect dist/server --watch dist/server\"",
    "build-server": "tsc -p tsconfig.server.json",
    "build-client": "webpack"
  },
  "repository": "https://github.com/jmarcelfpg/hanashichat.git",
  "keywords": [
    "chat",
    "leaf",
    "leaves"
  ],
  "author": "jmarcelfpg <jmarcel_fpg@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmarcelfpg/hanashichat/issues"
  },
  "homepage": "https://github.com/jmarcelfpg/hanashichat#readme",
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
