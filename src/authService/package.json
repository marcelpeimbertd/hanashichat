{
  "name": "hanashichat",
  "version": "0.0.5",
  "description": "A chat to interact with new people or old friends",
  "main": "/dist/server/index.js",
  "scripts": {
    "start-auth": "node -r dotenv/config authService/",
    "start-development:auth-only": "concurrently \"mongod \" \"tsc -p tsconfig.auth.server.json -w\" \"nodemon -r dotenv/config dist/authService --watch dist/authService\"",
    "start-development:auth-only:debug": "concurrently \"mongod \" \"tsc -p tsconfig.auth.server.json -w\" \"nodemon --inspect-brk -r dotenv/config dist/authService --watch dist/authService\"",
    "build-auth": "tsc -p tsconfig.auth.server.json"
  },
  "repository": "https://github.com/jmarcelfpg/hanashichat.git",
  "keywords": [
    "chat",
    "leaf",
    "leaves"
  ],
  "author": "jmarcelfpg <jmarcel_fpg@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmarcelfpg/hanashichat/issues"
  },
  "homepage": "https://github.com/jmarcelfpg/hanashichat#readme",
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "moment": "^2.22.1",
    "mongoose": "^5.1.0",
    "morgan": "^1.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
